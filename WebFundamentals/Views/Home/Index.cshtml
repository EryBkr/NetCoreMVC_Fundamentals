@model IEnumerable<WebFundamentals.Models.MusteriViewModel> 
@{
    Layout = "~/Views/Shared/_Layout.cshtml"; //Layout sayfasını tanımladık
}

@Html.Partial("_CategoryPartialView") @*Her sayfada model göndermemiz pek sağlıklı değil*@

    <p>
        Viewbag.Isim=<span>@ViewBag.Isim</span>
    </p>
    <p>
        Tempdata["Isim"]=<span>@TempData["Isim"]</span>
    </p>
    <p>
        Viewdata["Isim"]=<span>@ViewData["Isim"]</span>
    </p>
    @foreach (var item in Model)
    {
        <p>@item.Name</p>
    }
